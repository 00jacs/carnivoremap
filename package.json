		"@types/eslint": "^9.6.0",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"husky": "^9.1.5",
		"lint-staged": "^15.2.9",
		"postcss": "^8.4.41",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.6.6",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"svelte-radix": "^1.1.1",
		"tailwindcss": "^3.4.10",
