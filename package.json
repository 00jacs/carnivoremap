		"bits-ui": "^0.21.13",
		"clsx": "^2.1.1",
		"cmdk-sv": "^0.0.18",
		"mode-watcher": "^0.4.1",
		"tailwind-merge": "^2.5.2",
		"tailwind-variants": "^0.2.1"
	},
	"lint-staged": {
		"**/*.{svelte,js,ts,html,css}": [
			"prettier --write",
			"eslint --fix"
		]
	}
}
