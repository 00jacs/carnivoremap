{
  "useTabs": false,
  "tabWidth": 2,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 80,
  "htmlWhitespaceSensitivity": "ignore",
  "bracketSameLine": true,
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
